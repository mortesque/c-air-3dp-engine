module crnky(crp,od,msize,h){
$fn=64;
 translate([0,0,crp/2])
 difference(){
  cube(crp,center=true);
  translate([0,0,-crp/2])
  cylinder(h=crp,r=msize/2);   
 }
 translate([0,0,crp])
 difference(){
  cylinder(h=h-(crp*2),r=od/2);
  cylinder(h=h-(crp*2),r=msize/2);   
 }
 translate([0,0,crp/2+(h-(crp*2))+crp])
 difference(){
  cube(crp,center=true);
  translate([0,0,-crp/2])
  cylinder(h=crp,r=msize/2);   
 } 
}
